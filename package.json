    "@polymer/paper-toggle-button": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "@types/materialize-css": "^1.0.8",
    "@types/uuid": "^8.0.0",
    "globular-web-client": "^1.1.91",
